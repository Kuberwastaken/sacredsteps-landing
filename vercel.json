{
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist/public",
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/favicon/(.*)",
      "dest": "/client/favicon/$1"
    },
    {
      "src": "/(.*\\.(js|mjs|css|map|json|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot|webmanifest)$)",
      "dest": "/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
